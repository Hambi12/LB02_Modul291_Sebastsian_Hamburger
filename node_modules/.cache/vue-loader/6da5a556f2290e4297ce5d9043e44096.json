eCat\">- 1 Burger</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      newName:''\r\n    }\r\n  },\r\n  methods: {\r\n    setName() {\r\n      if(this.newName) {\r\n        this.$store.commit('setName', this.newName);\r\n        this.newName = '';\r\n      }\r\n    },\r\n    addCat() {\r\n      this.$store.commit('adoptCat');\r\n    },\r\n    removeCat() {\r\n      this.$store.commit('placeCat');\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}