v-if=\"true\" v-show=\"true\" v-text=\"subTitleSummer\">Loading...</h2>\n    <h2 v-else v-show=\"true\" v-text=\"subTitleWinter\">Loading...</h2>\n    <!-- price section -->\n    <h3>Warenkorb: (Artikel: {{ totalItems }}) {{ formatPrice(\"CHF\", totalCost) }} (CHF {{ discount }} off)</h3>\n    <a href=\"#\" @click=\"updatePrice\">Coupon einlösen</a>\n    <!-- menu section -->\n    <h1>Speisekarte:</h1>\n    <h2>Essen:</h2>\n    <div class=\"even-columns\" v-for=\"item in productsFood\" :key=\"item\">\n      <div class=\"col\">{{ item.title }}</div>\n      <div class=\"col\"><a href=\"#\" @click=\"addToCart(item.price)\">Add {{ formatPrice(item.currency, item.price) }}</a></div>\n    </div>\n    <h2>Getränke:</h2>\n    <div class=\"even-columns\" v-for=\"item in productsDrinks\" :key=\"item\">\n      <div class=\"col\">{{ item.title }}</div>\n      <div class=\"col\"><a href=\"#\" @click=\"addToCart(item.price)\">Add {{ formatPrice(item.currency, item.price) }}</a></div>\n    </div>\n    <!-- most popular products section -->\n    <h2>Beliebteste Produkte:</h2>\n    <ul>\n      <li v-for=\"(item, n) in mostPopularProducts\" :key=\"n\">\n        {{ item }}\n      </li>\n    </ul>\n    <!-- ads section -->\n    <h2>Werbung</h2>\n    <a v-if=\"true\"\n      :href=\"adSummer.url\"\n      :target=\"adSummer.target\"\n      :tabindex=\"adSummer.tabindex\"\n    />\n    <a v-else\n       :href=\"adWinter.url\"\n       :target=\"adWinter.target\"\n       :tabindex=\"adWinter.tabindex\"\n    />\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      // text data\n      mainTitle: 'Restaurant das isses new',\n      subTitleSummer: 'Jetzt Sommerrabatt-Coupon einlösen!!',\n      subTitleWinter: 'Jetzt Winterrabatt-Coupon einlösen!!',\n      adSummer: {\n        url: 'https://www.lipton.com/ch/de/home.html',\n        target: '_blank',\n        tabindex: '0',\n        title: 'Das ist Lipton Werbung',\n      },\n      adWinter: {\n        url: 'https://www.ovomaltine.ch/',\n        target: '_blank',\n        tabindex: '0',\n        title: 'Das ist Ovo Werbung',\n      },\n      // total data\n      totalItems: 0,\n      totalCost: 0,\n      productsFood: [\n        {title:\"Pizza\", currency: \"CHF\", price: 20.50},\n        {title:\"Burger\", currency: \"CHF\", price: 17.50},\n        {title:\"Döner\", currency: \"CHF\", price: 10.50},\n        {title:\"Schnitzel\", currency: \"CHF\", price: 14.50},\n      ],\n      productsDrinks: [\n        {title:\"IceTea\", currency: \"CHF\", price: 3.75},\n        {title:\"CocaCola\", currency: \"CHF\", price: 4.25},\n        {title:\"Wasser\", currency: \"CHF\", price: 2.00},\n      ],\n      // discount data\n      discount: 0,\n      // most popular products data\n      mostPopularProducts: ['Burger', 'Döner', 'IceTea'],\n    }\n  },\n  methods: {\n    addToCart(n) {\n      this.totalItems = this.totalItems + 1\n      this.totalCost = this.totalCost + n\n      this.discount--;\n    },\n    formatPrice(currency, val) {\n      return `${currency} ${val.toFixed(2)}`\n    },\n    updatePrice(){\n      if (this.totalCost < 1) return;\n      this.totalCost--;\n    }\n  },\n  watch: {\n    'totalCost'(newVal,oldVal){\n      this.discount++;\n      console.log(`actual(CHF ${newVal}) and old(CHF ${oldVal}) price.`);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.even-columns {\n  display: flex;\n  //gap: 0.1em;\n  padding: 0.2em;\n}\n\n.even-columns > * {\n  flex: 1;\n}\n\n.card {\n  padding: 2em;\n  background: var(--clr-gradient);\n  color: white;\n  border-radius: 0.25rem;\n}\n\n/* added to help visualize the widths\n.even-columns > *:nth-child(2n + 1) {\n  outline: 1px solid blue;\n}*/\n\n/* =========================\n   GENERAL STYLES\n   NOT RELATED TO THE TUTORIAL\n============================ */\n\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\n:root {\n  --clr-primary: #ee6352;\n  --clr-secondary: #d16e8d;\n  --clr-accent: #f7f7ff;\n  --clr-gradient: linear-gradient(\n          -90deg,\n          var(--clr-primary),\n          var(--clr-secondary)\n  );\n  --ff-title: bungee, sans-serif;\n  --ff-body: canada-type-gibson, sans-serif;\n  --fw-body: 300;\n  --fw-bold: 800;\n  --fw-title: 400;\n  --fw-number: 800;\n}\n\nbody {\n  padding: 2rem;\n  min-height: 80vh;\n  font-family: var(--ff-body);\n  font-weight: var(--fw-body);\n  font-size: 1.25rem;\n  display: flex;\n  flex-direction: column;\n}\n\na {\n  display: inline-block;\n  background: rgb(235, 50, 50);\n  border-radius: 10px;\n  font-size: 10px;\n  color: white;\n  padding: 5px 10px;\n  text-decoration: none;\n}\n.container {\n  width: min(95%, 30rem);\n  margin: 0 auto;\n}\n</style>\n"]}]}