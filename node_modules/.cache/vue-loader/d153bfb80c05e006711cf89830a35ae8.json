   <button @click=\"removeBurger\">- 1 Burger</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      newOrdernumber:''\r\n    }\r\n  },\r\n  methods: {\r\n    setOrdernumber() {\r\n      if(this.newOrdernumber) {\r\n        this.$store.commit('setOrdernumber', this.newOrdernumber);\r\n        this.newOrdernumber = '';\r\n      }\r\n    },\r\n    addBurger() {\r\n      this.$store.commit('addBurger2');\r\n    },\r\n    removeBurger() {\r\n      this.$store.commit('removeBurger2');\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}